{
    "/" : "./public",

    "/app.css" : {
	    "build": "./gh-pages/app.css",
        "input": [
            "./css/reset.css",
            "./css/main.styl",
            "./css/jquery.mobile-1.1.0-rc.1.min.css"
        ]
    },

    "/app_android.css" : {
	    "build": "./android/assets/www/app.css",
        "input": [
            "./css/reset.css",
            "./css/main.styl",
            "./css/jquery.mobile-1.1.0-rc.1.min.css"
        ]
    },

    "/app.js" : {
        "build" : "./gh-pages/app.js",
        "input" : {
            "dependency": [
                "spine"
            ],
            "lib" : [
                "./lib/es5-shim.js",
                "./lib/jquery-1.7.1.min.js",
                "./lib/css3-mediaqueries.js",
                "./lib/html5shim.js"
            ],
            "module": [ "./coffee" ]
        }
    },


    "/app_android.js" : {
        "build" : "./android/assets/www/app.js",
        "input" : {
            "dependency": [
                "spine"
            ],
            "lib" : [
                "./lib/es5-shim.js",
                "./lib/jquery-1.7.1.min.js"
            ],
            "module": [ "./coffee" ]
        }
    }

}
